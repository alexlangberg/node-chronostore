{
  "name": "chronostore",
  "version": "1.2.0",
  "description": "Time-based, streaming file system.",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node test/*.js | faucet && npm run clean",
    "lint": "eslint lib test || true",
    "coverage": "covert test/*.js || true && npm run clean",
    "coveralls": "npm run coverage -s && coveralls < coverage/lcov.info",
    "watch": "watch 'clear && npm run lint && npm test && npm run coverage' lib test",
    "doctoc": "doctoc --github --title \"# Changelog\" CHANGELOG.md",
    "patch": "npm version patch",
    "minor": "npm version minor",
    "major": "npm version major",
    "clean": "rimraf testdata chronostore"
  },
  "dependencies": {
    "gulp": "^3.9.1",
    "gulp-filter": "^4.0.0",
    "gulp-gunzip": "0.0.3",
    "gulp-gzip": "^1.2.0",
    "gulp-if": "^2.0.0",
    "gulp-rename": "^1.2.2",
    "gulp-util": "^3.0.7",
    "moment": "^2.13.0",
    "node-uuid": "^1.4.7",
    "ramda": "^0.21.0",
    "readdirp": "^2.0.0",
    "rotating-file-stream": "^1.0.4",
    "through2": "^2.0.1",
    "vinyl": "^1.1.1"
  },
  "devDependencies": {
    "concat-stream": "^1.5.1",
    "coveralls": "^2.11.9",
    "covert": "^1.1.0",
    "doctoc": "^1.0.0",
    "eslint": "^2.8.0",
    "faucet": "0.0.1",
    "fs-extra": "^0.28.0",
    "rimraf": "^2.5.2",
    "tape": "^4.5.1",
    "watch": "^0.17.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexlangberg/node-chronostore.git"
  },
  "keywords": [
    "chronological",
    "time",
    "storage",
    "files",
    "filesystem"
  ],
  "author": "Alex Langberg",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexlangberg/node-chronostore/issues"
  },
  "homepage": "https://github.com/alexlangberg/node-chronostore#readme"
}
